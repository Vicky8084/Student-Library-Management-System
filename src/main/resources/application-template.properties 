spring.application.name=student-library-management
server.port=8010

# MySQL Datasource - Update with your credentials
spring.datasource.url=jdbc:mysql://localhost:3306/student_library_management
spring.datasource.username=YOUR_USERNAME
spring.datasource.password=YOUR_PASSWORD
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# JPA Configuration
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=update

